{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*).(js|css|png|jpg|jpeg|gif|svg|ico)$",
            "dest": "/$1.$2",
            "continue": true
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}